{
  "name": "foodies",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:prod": "next dev",
    "dev": "dotenv -e .env.local next dev",
    "build": "next build",
    "start": "next start",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx --fix --cache",
    "lint-default": "next lint",
    "prepare": "husky",
    "test": "npx jest --passWithNoTests",
    "madge": "npx madge --extensions js,jsx,ts,tsx .",
    "format": "npx prettier --check .",
    "init-db": "dotenv -e .env.local node db/init/postgres.mjs",
    "init-db-prod": "node db/init/postgres.mjs"
  },
  "dependencies": {
    "@vercel/blob": "^0.22.3",
    "basic-ftp": "^5.0.5",
    "better-sqlite3": "^9.4.3",
    "fs": "^0.0.1-security",
    "isomorphic-dompurify": "^2.7.0",
    "next": "14.2.2",
    "pg": "^8.11.5",
    "react": "^18",
    "react-dom": "^18",
    "react-toastify": "^10.0.5",
    "slugify": "^1.6.6",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.1.4",
    "@types/dompurify": "^3.0.5",
    "@types/node": "20.11.26",
    "@types/react": "18.2.64",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "dotenv-cli": "^7.4.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "14.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "madge": "^6.1.0",
    "prettier": "^3.2.5"
  }
}
